{
  "name": "wallmanderco/elasticsearch-indexer",
  "description": "Elasticsearch indexer for WordPress and WooCommerce",
  "license": "GPLv2",
  "keywords": ["wordpress", "wordpress-plugin", "elasticsearch", "indexer", "search", "performance"],
  "authors": [
    {
      "name": "Mikael Mattsson",
      "email": "mikael@wallmanderco.se"
    }
  ],
  "require": {
    "php": ">=5.4.7",
    "elasticsearch/elasticsearch": "~1.0"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "2.0.*@dev"
  },
  "autoload": {
    "psr-4": {
      "Wallmander\\ElasticsearchIndexer\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  }
}
